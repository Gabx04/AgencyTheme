						p.proyecto website design
				.col-xs-12.col-sm-6.col-md-4
					.portfolio-imagen
						img(href="img/portfolio/escape.png")
					.portfolio-proyecto
						h5 escape
						p.proyecto website design
				.col-xs-12.col-sm-6.col-md-4
					.portfolio-imagen
						img(href="img/portfolio/dreams.png")
					.portfolio-proyecto
						h5 dreams
						p.proyecto website design
	section#about
		.container
			.row
				.col-xs-12
					h2 about
					p.subtitulo #{subtitulo}
			#timeline
				.timeline-block
					.timeline-img
						img(src="img/about/1.jpg", alt="picture")
					.timeline-content
						p.fecha 2009-2011
						p.acontecimiento our humble beginnings
						p #{textotimeline}
				.timeline-block
					.timeline-img
						img(src="img/about/2.jpg", alt="picture")
					.timeline-content
						p.fecha march 2011
						p.acontecimiento an agency is born
						p #{textotimeline}
				.timeline-block
					.timeline-img
						img(src="img/about/3.jpg", alt="picture")
					.timeline-content
						p.fecha december 2012
						p.acontecimiento transition to full service
						p #{textotimeline}
				.timeline-block
					.timeline-img
						img(src="img/about/4.jpg", alt="picture")
					.timeline-content
						p.fecha july 2014
						p.acontecimiento phase two expansion
						p #{textotimeline}
				.timeline-block
					.timeline-img
						p.endtimeline be part of our story
	section#team
		.container
			.row
				.col-xs-12
					h2 about
					p.subtitulo #{subtitulo}
			.row
				.col-xs-12.col-md-4
					.fotoperfil
						img(src="img/team/.jpg", alt="foto perfil equipo")
					p.nombre
					p.cargo
					.redes
						a(href="")





