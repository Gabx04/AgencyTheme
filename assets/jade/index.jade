	section#portfolio
		.container
			.row
				.col-xs-12
					h2 portfolio
					p.subtitulo #{subtitulo}
			.row
				.col-xs-12.col-sm-6.col-md-4
					.portfolio-imagen
						img(href="img/portfolio/roundicons.png")
					.portfolio-proyecto
						h5 round icons
						p.proyecto graphic design
				.col-xs-12.col-sm-6.col-md-4
					.portfolio-imagen
						img(href="img/portfolio/startup-framework.png")
					.portfolio-proyecto
						h5 startup framework
						p.proyecto website design 
				.col-xs-12.col-sm-6.col-md-4
					.portfolio-imagen
						img(href="img/portfolio/treehouse.png")
					.portfolio-proyecto
						h5 treehouse
						p.proyecto website design
				.col-xs-12.col-sm-6.col-md-4
					.portfolio-imagen
						img(href="img/portfolio/golden.png")
