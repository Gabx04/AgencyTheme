<!DOCTYPE html>
html(lang="en")
head
	meta(charset="UTF-8")
	meta( name='viewport', content='width=device-width, initial-scale=1.0' )
	title Agency Theme
	link(href="https://fonts.googleapis.com/css?family=Kaushan+Script", rel="stylesheet", type="text/css")
	link(href="https://fonts.googleapis.com/css?family=Montserrat:400,700", rel="stylesheet", type="text/css")
	link(href="https://fonts.googleapis.com/css?family=Droid+Serif:400,400italic", rel="stylesheet", type="text/css")
	link(href="https://fonts.googleapis.com/css?family=Roboto+Slab:400,300", rel="stylesheet", type="text/css")
	link(href="assets/font-awesome/css/font-awesome.min.css", rel="stylesheet")
	link(href="assets/css/bootstrap.css", rel="stylesheet")
	link(href="assets/css/main.css", rel="stylesheet")
body
	include ./_variables
	include ./_mixins

	nav.navbar.navbar-default.navbar-fixed-top
		.container
			.navbar-header.page-scroll
				button.navbar-toggle.collapsed( type="button", data-toggle="collapse", data-target="#navbar-collapse-1")
					span.sr-only Toggle navigation
					span.icon-bar
					span.icon-bar
					span.icon-bar
				a.navbar-brand.page-scroll(href="#header") star bootstrap
			#navbar-collapse-1.collapse.navbar-collapse
				ul.nav.navbar-nav.navbar-right
					li
						a(href="#services").page-scroll services
					li
						a(href="#portfolio").page-scroll portfolio
					li
						a(href="#about").page-scroll about
					li
						a(href="#team").page-scroll team
					li
						a(href="#contact").page-scroll contact
	header#header
		.container
			h1 welcome to our studio
			p it's nice to meet you
			a(href="#services").page-scroll tell me more
	section#services
		.container
			.row
				+inicioseccion("services")
			.row
				.col-xs-12.col-md-4.servicios
					+iconoservicio("fa-shopping-cart")
					+sobreservicio("e-comerce")
				.col-xs-12.col-md-4.servicios
					+iconoservicio("fa-laptop")
					+sobreservicio("responsive design")
				.col-xs-12.col-md-4.servicios
					+iconoservicio("fa-lock")
					+sobreservicio("web security")
	section#portfolio
		.container
			.row
				+inicioseccion("portfolio")
			.row
				.col-xs-12.col-sm-6.col-md-4
					+portfolio("img/portfolio/roundicons.png", "round icons", "graphic design")
				.col-xs-12.col-sm-6.col-md-4
					+portfolio("img/portfolio/startup-framework.png", "startup framework", "website design")
				.col-xs-12.col-sm-6.col-md-4
					+portfolio("img/portfolio/treehouse.png", "treehouse", "website design")
				.col-xs-12.col-sm-6.col-md-4
					+portfolio("img/portfolio/golden.png", "golden", "website design")
				.col-xs-12.col-sm-6.col-md-4
					+portfolio("img/portfolio/escape.png", "escape", "website design")
				.col-xs-12.col-sm-6.col-md-4
					+portfolio("img/portfolio/dreams.png", "dreams", "website design")
	section#about
		.container
			.row
				+inicioseccion("about")
			#timeline.row
				+hits("img/about/1.jpg", "2009-2011", "our humble beginnings")
				+hits("img/about/2.jpg", "march 2011", "an agency is born")
				+hits("img/about/3.jpg", "december 2012", "transition to full service")
				+hits("img/about/4.jpg", "july 2014", "phase two expansion")
				.timeline-block
					.timeline-img
						p.endtimeline be part of our story!
	section#team
		.container
			.row
				+inicioseccion("our amazing team")
			.row
				.col-xs-12.col-md-4.text-center
					+miembrosequipo("img/team/1.jpg", "Kay Garland", "Lead Designer")
				.col-xs-12.col-md-4.text-center
					+miembrosequipo("img/team/2.jpg", "Larry Parker", "Lead Marketer")
				.col-xs-12.col-md-4.text-center
					+miembrosequipo("img/team/3.jpg", "Diana Pertersen", "Lead Developer")
			.row
				p.team Lorem ipsum dolor sit amet, consectetur adipisicing elit. Voluptate numquam distinctio error dolore enim! Omnis explicabo cum ex eligendi, consequuntur nesciunt beatae, dolorem.
	section#marcas
		.container
			.row
				.col-xs-12.col-sm-6.col-md-3.text-center
					a(href="#")
						img(src="img/logos/envato.jpg", alt="logo envato")
				.col-xs-12.col-sm-6.col-md-3.text-center
					a(href="#")
						img(src="img/logos/designmodo.jpg", alt="logo designmodo")
				.col-xs-12.col-sm-6.col-md-3.text-center
					a(href="#")
						img(src="img/logos/themeforest.jpg", alt="logo themeforest")
				.col-xs-12.col-sm-6.col-md-3.text-center
					a(href="#")
						img(src="img/logos/creative-market.jpg", alt="logo creative market")
	section#contact
		.container
			.row
				+inicioseccion("contact us")
			.row
				.col-xs-12.col-md-6.col-contact
					form(action="")#usrform
						input(type="text", name="name", value="", placeholder="YOUR NAME*")
						input(type="text", name="email", value="", placeholder="YOUR EMAIL*")
						input(type="text", name="phone", value="", placeholder="YOUR PHONE*")
				.col-xs-12.col-md-6.col-contact
					textarea(name="message", placeholder="YOUR MESSAGE*", form="usrform")
			.row.text-center
				input(type="submit", value="SEND MESSAGE").send
	footer
		.container
			.row
				.col-xs-12.col-sm-4.col-footer
					p.credits Copyright Â© Your Website 2014
				.col-xs-12.col-sm-4.col-footer
					+redessociales
				.col-xs-12.col-sm-4.col-footer
					a(href="#").info Privacy Policy
					a(href="#").info Terms of Use
	script(src="assets/js/jquery.js")
	script(src="assets/js/bootstrap.js")
	script(src="assets/js/jquery.easing.1.3.js")
	script(src="assets/js/classie.js")
	script(src="assets/js/cbpAnimatedHeader.js")
	script(src="assets/js/jqBootstrapValidation.js")
	script(src="assets/js/contact_me.js")
	script(src="assets/js/agency.js")